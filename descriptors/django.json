{
	"name": "django",
	"mig": {
		"filename": "__init__.py",
		"filecontent": "^VERSION =",
		"filepath": "/usr/lib",
		"target": "tags->>'operator'='IT'",
		"postfilter": "django",
		"expiry": "30s"
	},
	"ssh": {
		"action": "egrep",
		"pattern": "'^VERSION = \\(\\S+, \\S+, \\S+, \\S+, \\S+\\)'",
		"outputtransform": "django-python"
	}
}
