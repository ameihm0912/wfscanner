{
	"name": "django",
	"mig": {
		"filename": "__init__.py",
		"filecontent": "^VERSION =",
		"filepath": "/",
		"target": "tags->>'operator'='IT'",
		"postfilter": "django",
		"expiry": "60s"
	},
	"ssh": {
		"action": "egrep",
		"pattern": "'^VERSION = \\([^ ]+, [^ ]+, [^ ]+, [^ ]+, [^ ]\\)$'",
		"outputtransform": "django-python"
	}
}
