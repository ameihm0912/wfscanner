{
	"name": "mediawiki",
	"mig": {
		"filename": "PHPVersionError.php",
		"filecontent": "mwVersion =",
		"filepath": "/",
		"target": "tags->>'operator'='IT'",
		"postfilter": "",
		"expiry": "60s"
	},
	"ssh": {
		"action": "egrep",
		"pattern": "'mwVersion ='",
		"outputtransform": "mediawiki-mwver"
	}
}
